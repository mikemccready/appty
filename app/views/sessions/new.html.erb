<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>
<%#= ENV["facebook_secret"] %>
<%#= ENV["GOOGLE_CLIENT_SECRET"] %>

<%= link_to "Authenticate with google!", "/auth/google_oauth2" %>


<h1>Log In</h1>
<%= form_for :login do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.submit 'Log In' %>
  </div>
<% end %>
